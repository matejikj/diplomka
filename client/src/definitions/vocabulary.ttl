wikimind:AccessControlPolicyType
    a rdfs:Class ;
    rdfs:comment "A resource containing information about a reservation state change."@en ;
    rdfs:label "ReservationStateChange"@en .

wikimind:LinkType
    a rdfs:Class ;
    rdfs:comment "A resource containing information about a reservation state change."@en ;
    rdfs:label "ReservationStateChange"@en .

### Enumerates
wikimind:ACL a wikimind:AccessControlPolicyType ;
    rdfs:label "ACL"@en ;
    rdfs:comment "ACL access control policy for solid pod"@en .

wikimind:WAC a wikimind:AccessControlPolicyType ;
    rdfs:label "WAC"@en ;
    rdfs:comment "WAC access control policy for solid pod"@en .

wikimind:GRAPH a wikimind:LinkType ;
    rdfs:label "GRAPH link type"@en ;
    rdfs:comment "GRAPH"@en .

wikimind:CLASS a wikimind:LinkType ;
    rdfs:label "CLASS link type"@en ;
    rdfs:comment "CLASS"@en .

wikimind:PROFILE a wikimind:LinkType ;
    rdfs:label "PROFILE link type"@en ;
    rdfs:comment "PROFILE"@en .

wikimind:CHAT a wikimind:LinkType ;
    rdfs:label "CHAT link type"@en ;
    rdfs:comment "CHAT"@en .

### Properties
solid:webId
    a rdf:Property ;
    rdfs:comment "WebID of user."@en ;
    rdfs:label "webId"@en ;

solid:profileImage
    a rdf:Property ;
    rdfs:comment "Link to profile image."@en ;
    rdfs:label "profileImage"@en ;




wikimind:isProcessed
    a rdf:Property ;
    rdfs:comment "Defines if the subject has been processed already."@en ;
    rdfs:label "isProcessed"@en .

wikimind:notificationType
    a rdf:Property ;
    rdfs:comment "Defines the type of the notification"@en ;
    rdfs:label "notificationType"@en ;
    schema:domainIncludes solid:Notification ;
    schema:rangeIncludes wikimind:NotificationType .




