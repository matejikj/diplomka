    rdfs:label "ReservationStateChange"@en .

wikimind:Message
    a rdfs:Class ;
    rdfs:comment "A resource containing information about a reservation state change."@en ;
    rdfs:label "ReservationStateChange"@en .

wikimind:MindMap
    a rdfs:Class ;
    rdfs:comment "A resource containing information about a reservation state change."@en ;
    rdfs:label "ReservationStateChange"@en .

wikimind:Node
    a rdfs:Class ;
    rdfs:comment "A resource containing information about a reservation state change."@en ;
